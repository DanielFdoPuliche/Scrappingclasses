{
    "manifest_version": 3,
    "name": "My Scrapper",
    "version": "1.0.0",
    "background":{
        "service_worker":"background.js"
    },
    "permissions":["activeTab","scripting"],
    "commands":{
        "reload":{
            "suggested_key":"Ctrl+Shift+Y",
            "description":"Reload the extension"
        }
    },
    "action":{
        "default_title":"Click me"
    }
}